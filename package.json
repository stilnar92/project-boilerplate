{
  "name": "project-boilerplate",
  "version": "0.1.0",
  "description": "React project config",
  "keywords": [
    "Example",
    "Keywords"
  ],
  "homepage": "https://github.com/stilnar92/project-boilerplate",
  "bugs": {
    "email": "stilnar92@gmail.com",
    "url": "https://github.com/stilnar92/project-boilerplate//issues"
  },
  "author": "Ilnar Gatiyatullin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stilnar92/project-boilerplate.git"
  },
  "private": true,
  "scripts": {
    "check-staged": "lint-staged",
    "lint": "eslint --fix config -c .eslintrc --ext js,jsx",
    "prettier": "prettier  --write",
    "build:prod": "NODE_ENV=production webpack-cli",
    "dev": "webpack-dev-server",
    "eslint": "eslint --init"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json, eslintrc}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "path": "^0.12.7",
    "prettier": "^1.16.1",
    "react-hot-loader": "^4.6.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-notifier": "^1.7.0"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
